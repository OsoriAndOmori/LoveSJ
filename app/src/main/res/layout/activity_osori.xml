<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".activity.OsoriActivity">

	<TextView
		android:id="@+id/love_dday"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="연애 Dday!"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/wedding_dday"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="결혼 Dday!"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintVertical_bias="0.423" />

	<Button
		android:id="@+id/kh_family"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="46dp"
		android:layout_marginEnd="16dp"
		android:text="Button"
		app:layout_constraintEnd_toStartOf="@+id/love_dday"
		app:layout_constraintTop_toBottomOf="@+id/love_dday" />

	<Button
		android:id="@+id/sj_family"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginTop="46dp"
		android:layout_marginEnd="65dp"
		android:text="Button"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/love_dday" />

</android.support.constraint.ConstraintLayout>